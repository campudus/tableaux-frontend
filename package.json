{
  "name": "grud-frontend",
  "version": "1.0.0",
  "author": "Campudus GmbH",
  "description": "A web interface for tableaux. A restful service for storing data in tables.",
  "main": "index.js",
  "scripts": {
    "start": "export NODE_ENV=production; export BUILD_ID=$(./getCommitHash.sh); node out/grud-frontend-server.js",
    "build": "export NODE_ENV=production; parcel build src/index.html src/worker.js --out-dir out",
    "dev": "export NODE_ENV=development; export BUILD_ID=$(./getCommitHash.sh); node dev-server.js",
    "lint": "eslint -f stylish --ignore-pattern src/scss/FontAwesome \"src/**/*.{js,jsx}\"",
    "lint:changes": "git diff master --name-only | grep -E \"src\\/.*\\.jsx?$\" | xargs -I{} sh -c \"test -f '{}' && echo '{}'\" | xargs eslint",
    "lint:fix": "eslint --fix -f stylish --ignore-pattern src/scss/FontAwesome \"src/**/*.{js,jsx}\"",
    "lint:fix:changes": "git diff master --name-only | grep -E \"src\\/.*\\.jsx?$\" | xargs -I{} sh -c \"test -f '{}' && echo '{}'\" | xargs eslint --fix",
    "storybook": "start-storybook -p 3001 -s src",
    "test": "NODE_ICU_DATA=node_modules/full-icu jest",
    "test:ci": "JEST_JUNIT_OUTPUT=./output/coverage/junit.xml NODE_ICU_DATA=node_modules/full-icu jest --ci --reporters=default --reporters=jest-junit",
    "clean": "rm -fr .cache out",
    "clean:project": "rm -fr .cache out node_modules; npm install && npm ci"
  },
  "staticFiles": {
    "staticPath": "src/static"
  },
  "license": "Apache license 2.0",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@sentry/browser": "^4.6.4",
    "@storybook/addon-centered": "^3.2.15",
    "@storybook/addon-info": "^3.3.13",
    "@storybook/addon-knobs": "^4.1.13",
    "@storybook/react": "^3.4.11",
