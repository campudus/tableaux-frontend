{
  "name": "grud-frontend",
  "version": "1.0.0",
  "author": "Campudus GmbH",
  "description": "A web interface for tableaux. A restful service for storing data in tables.",
  "type": "module",
  "scripts": {
    "start": "export NODE_ENV=production; export BUILD_ID=${BUILD_ID:-$(./getCommitHash.sh)}; node server/server.js",
    "build": "export NODE_ENV=production; export BUILD_ID=${BUILD_ID:-$(./getCommitHash.sh)}; tsc --noEmit && vite build",
    "dev": "export NODE_ENV=development; export BUILD_ID=${BUILD_ID:-$(./getCommitHash.sh)}; node server/server.js --dev",
    "lint": "eslint -f stylish \"{src,server}/**/*.{js,jsx,ts,tsx}\"",
    "lint:changes": "git diff master --name-only | grep -E \"src\\/.*\\.(j|t)sx?$\" | xargs -I{} sh -c \"test -f '{}' && echo '{}'\" | xargs eslint",
    "lint:fix": "eslint --fix -f stylish \"{src,server}/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix:changes": "git diff master --name-only | grep -E \"src\\/.*\\.(j|t)sx?$\" | xargs -I{} sh -c \"test -f '{}' && echo '{}'\" | xargs eslint --fix",
    "test": "NODE_ICU_DATA=./node_modules/full-icu vitest run",
    "test:ci": "NODE_ICU_DATA=./node_modules/full-icu vitest run --coverage",
    "clean": "rm -fr .cache out",
    "clean:project": "rm -fr .cache out node_modules; npm install && npm ci"
  },
